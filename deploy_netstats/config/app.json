[
  {
    "name"              : "{{ nodename }}",
    "script"            : "app.js",
    "log_date_format"   : "YYYY-MM-DD HH:mm Z",
    "merge_logs"        : false,
    "watch"             : false,
    "max_restarts"      : 10,
    "exec_interpreter"  : "node",
    "exec_mode"         : "fork_mode",
    "env":
    {
      "NODE_ENV"        : "production",
      "RPC_HOST"        : "localhost",
      "RPC_PORT"        : "{{rpc_port}}",
      "LISTENING_PORT"  : "{{listen_port}}",
      "INSTANCE_NAME"   : "{{ nodename }}",
      "CONTACT_DETAILS" : "",
      "WS_SERVER"       : "{{netstats_ws_host}}",
      "WS_SECRET"       : "{{netstats_ws_secret}}",
      "VERBOSITY"       : 2
    }
  }
]